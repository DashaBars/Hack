local a=loadstring(game:HttpGet('https://raw.githubusercontent.com/DashaBars/Hack/main/Black%20GUI'))()local b=a:CreateWindow("Driving Empire")local c=b:CreateFolder("Farm")c:Toggle("Collect Pumpkins",function(d)Pum=d end)spawn(function()while wait()do if Pum==true then game:GetService("Workspace").Game.Halloween.Pumpkins['_p'].CFrame=game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame+Vector3.new(0,7,0)end end end)c:Toggle("Tp to Pumpkins",function(d)Tp=d end)spawn(function()while wait()do if Tp==true then game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").Game.Halloween.Pumpkins['_p'].CFrame+Vector3.new(0,7,0)end end end)
