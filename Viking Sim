local a=loadstring(game:HttpGet('https://raw.githubusercontent.com/DashaBars/Hack/main/Black%20GUI'))()local b=a:CreateWindow("Viking Sim")local c=b:CreateFolder("Areas")local d=b:CreateFolder("Farm")local e=b:CreateFolder("Misc")SelectedArea="None"TPs={}for f,g in pairs(game:GetService("Workspace")["SpawnPoints"]:GetChildren())do table.insert(TPs,g.Name)end;c:Dropdown("Select Area",TPs,true,function(d)SelectArea=d end)c:Button("Teleports",function()game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=game:GetService("Workspace").SpawnPoints[SelectArea].Spawn.CFrame+Vector3.new(0,5,0)end)d:Toggle("Farm-Coins",function(h)Coins=h end)game:GetService('RunService').Stepped:connect(function()spawn(function()pcall(function()if Coins==true then for f,g in pairs(game:GetService("Workspace")['Coins']:GetChildren())do g.CFrame=game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame end end end)end)end)e:Toggle("Anti-AFK",function(h)AntiAFK=h end)spawn(function()if AntiAFK==true then local i=game:GetService("VirtualUser")game:GetService("Players").LocalPlayer.Idled:connect(function()i:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)wait(1)i:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)end)end end)e:Button("Gamepasses",function()game:GetService("Players").LocalPlayer.Gamepasses['2xSpeed'].Value=true;game:GetService("Players").LocalPlayer.Gamepasses['InfArrows'].Value=true;game:GetService("Players").LocalPlayer.Gamepasses['2xPlunder'].Value=true;game:GetService("Players").LocalPlayer.Gamepasses['PetInv'].Value=true;game:GetService("Players").LocalPlayer.Gamepasses['2xDamage'].Value=true;game:GetService("Players").LocalPlayer.Gamepasses['2xCoins'].Value=true;game:GetService("Players").LocalPlayer.Gamepasses['2xHealth'].Value=true;game:GetService("Players").LocalPlayer.Gamepasses['FasterSword'].Value=true;game:GetService("Players").LocalPlayer.Gamepasses['InfBackpack'].Value=true;game:GetService("Players").LocalPlayer.Gamepasses['PetEquip'].Value=true end)
