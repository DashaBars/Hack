		local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/DashaBars/Hack/main/Rainbow%20GUI%20Tab", true))()

		local tab1 = library:CreateTab("NPC Words", true)
		
	local button = library:MakeButton(tab1,"Today is my birthday!",function(button)
  	local args = {
  	[1] = "Today is my birthday!",
  	[2] = "All"
	}
	game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
	end)

	local button = library:MakeButton(tab1,"Ban Taco!",function(button)
